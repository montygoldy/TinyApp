<!DOCTYPE Html>
<html>
  <head>
    <title>TinyApp</title>
  </head>
  <body>

    <% include partials/_header %>

    <ul>
      <% for ( url in urls){ %>
        <li><%= url %></li>
        <li><%= urls[url].longURL %></li>
    </ul>

    <a href ="/urls/<%= url %>">Edit</a>

    <form action="/urls/<%= url %>/delete" method="post">
        <input type="submit" value="Delete" name="delete">
    </form>
    <%  } %>


  </body>
</html>